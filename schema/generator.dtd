<?xml version='1.0' encoding='UTF-8'?>
		<!ENTITY % lang "lang CDATA '*'">

		<!ELEMENT model (class|iface|entity|include|types)*>

		<!ELEMENT entity (implements|property|method|extends|const|doc|annotate)*>
		<!ATTLIST entity
				%lang;
				type CDATA #REQUIRED
				templateId CDATA #REQUIRED
                exclude (true|false) "false">

		<!ELEMENT class (implements|property|method|extends|const|decorates|doc|annotate)*>
		<!ATTLIST class
				%lang;
				type CDATA #REQUIRED
				templateId CDATA "class"
                exclude (true|false) "false">

		<!ELEMENT iface (property|method|extends|doc|annotate)*>
		<!ATTLIST iface
				%lang;
				type CDATA #REQUIRED
				templateId CDATA "iface"
                exclude (true|false) "false">

		<!ELEMENT property (doc|annotate)*>
		<!ATTLIST property
				%lang;
				name CDATA #REQUIRED
				type CDATA #REQUIRED
                readOnly (true|false) "false">

		<!ELEMENT const (#PCDATA)>
		<!ATTLIST const
				%lang;
				name CDATA #REQUIRED
				type CDATA #REQUIRED>

		<!ELEMENT doc (#PCDATA)>
		<!ATTLIST doc
				%lang;>

        <!ELEMENT annotate (#PCDATA)>
        <!ATTLIST annotate
                %lang;>

		<!ELEMENT extends EMPTY>
		<!ATTLIST extends
				%lang;
				type CDATA #REQUIRED>

		<!ELEMENT decorates EMPTY>
		<!ATTLIST decorates
				%lang;
				type CDATA #REQUIRED
				name CDATA #REQUIRED
				prefix CDATA ""
                readOnly (true|false) "false">

		<!ELEMENT method (param|doc|annotate)*>
		<!ATTLIST method
				%lang;
				name CDATA #REQUIRED
				returns CDATA #REQUIRED>

		<!ELEMENT param EMPTY>
		<!ATTLIST param
				type CDATA #REQUIRED
				name CDATA #REQUIRED>

		<!ELEMENT implements EMPTY>
		<!ATTLIST implements
				%lang;
				iface CDATA #REQUIRED>

		<!ELEMENT include EMPTY>
		<!ATTLIST include
				path CDATA #REQUIRED>

		<!ELEMENT types (type)*>

		<!ELEMENT type (target)*>
		<!ATTLIST type
				name CDATA #REQUIRED>

		<!ELEMENT target EMPTY>
		<!ATTLIST target
				%lang;
				value CDATA #REQUIRED>